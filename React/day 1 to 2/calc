class cal1{
  constructor(a,b){
    this.a=a;
    this.b=b;
  }
  add(){
    console.log(this.a+this.b);
  }
  sub(){
    console.log(this.a-this.b)
  }
  mul(){
    console.log(this.a*this.b)
  }
  div(){
    console.log(this.a/this.b)
  }
  }
calc=new cal1(12,19);
calc.add();
calc.sub();
calc.mul();
calc.div();
